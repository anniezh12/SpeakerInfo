<H1>Speakers</h1>
  <form method=post action="#" id="noteForm">
     <div class="form-group">
  <input type=textarea class="form-control" id="notes" >
  <input type=submit value="Submit">
</div>
  </form>
  <div id="messageDiv"></div>

  <% User.all.each do |user| %>
  <table>
   <tr><td><%= user.name %></td>
   <td>&nbsp;&nbsp;&nbsp;
         <% if current_user && user.id == current_user.id %>
         <%= link_to "More About Speaker", user_path(user),method: :get%>/
         <%= link_to "Edit", edit_user_path(user),method: :get%>
         <% else %>
         <%= link_to "More About Speaker", user_path(user),method: :get%>
         <% end %>
    </td>
   <%end %>
 </table>
