<H1>All Speakers</h1>
  <% @users.each do |user| %>
  <br>
   <%= user.id %>
   <% if current_user && user.id == current_user.id %>
   <%= button_to "Edit", edit_user_path(user),method: :get%>
   <% else %>
   <%= button_to "More About Speaker", user_path(user),method: :get%>
   <% end %>


   <%end %>
<%= button_to "Sign Out",destroy_user_session_path, method: :delete%>
